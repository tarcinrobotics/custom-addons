<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--define a view tree -->
        <record id="qualification_student_view_tree" model="ir.ui.view">
            <field name="name">view_qualification_student_tree</field>
            <field name="model">qualification.student</field>
            <field name="arch" type="xml">
                <tree multi_edit="1" editable="bottom">
                    <field name="name" />
                    <field name="alternative_name" />
                </tree>
            </field>
        </record>

        <!--define a view action -->
        <record id="qualification_student_action" model="ir.actions.act_window">
            <field name="name">Qualification Student</field>
            <field name="res_model">qualification.student</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>



